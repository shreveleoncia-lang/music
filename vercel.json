{
  "版本": 2,
  "名称": "我的音乐网站",
  "生成": [
    {
      "src": "*.html",
      "使用": "@Vercel/static"
    },
    {
      "src": "*.css",
      "使用": "@Vercel/static"
    },
    {
      "src": "*.js",
      "使用": "@Vercel/static"
    },
    {
      "src": "*.json",
      "使用": "@Vercel/static"
    }
  ],
  "路由": [
    {
      "src": "/(.*\\.(html|css|js|json))",
      "目的地": "/$1"
    },
    {
      "src": "/(.*)",
      "目的地": "/index.html"
    }
  ]
}
